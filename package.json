{
  "name": "toggl-overtime-tracker",
  "productName": "Toggl Overtime Tracker",
  "version": "0.0.1",
  "description": "Manage your overtime using Toggl time entries",
  "author": "hendrik Bulens <hendrik.bulens@gmail.com> (https://www.hendrikbulens.com/)",
  "private": false,
  "main": "public/electron.js",
  "dependencies": {
    "@material-ui/core": "4.3.2",
    "@material-ui/icons": "4.2.1",
    "bluebird": "^3.7.1",
    "chartist": "0.10.1",
    "classnames": "2.2.6",
    "date-fns": "^2.5.1",
    "font-awesome": "^4.7.0",
    "history": "4.9.0",
    "lodash": "^4.17.15",
    "material-icons": "^0.3.1",
    "material-table": "^1.52.0",
    "moment": "^2.24.0",
    "moment-range": "^4.0.2",
    "nedb": "^1.8.0",
    "perfect-scrollbar": "1.4.0",
    "prop-types": "15.7.2",
    "query-string": "^6.8.3",
    "react": "16.9.0",
    "react-chartist": "0.13.3",
    "react-dom": "16.9.0",
    "react-google-maps": "9.4.5",
    "react-router-dom": "5.0.1",
    "react-scripts": "3.1.0",
    "react-swipeable-views": "0.13.3"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "install:clean": "rm -rf node_modules/ && rm -rf package-lock.json && npm install && npm start",
    "lint:check": "eslint . --ext=js,jsx;  exit 0",
    "lint:fix": "eslint . --ext=js,jsx --fix;  exit 0",
    "build-package-css": "cp src/assets/css/material-dashboard-react.css dist/material-dashboard-react.css",
    "build-package": "npm run build-package-css && babel src --out-dir dist",
    "electron": "electron .",
    "dev": "nf start",
    "package-win": "yarn build && electron-builder build --win --x64"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hbulens/overtime-tracker.git"
  },
  "keywords": [],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hbulens/overtime-tracker/issues"
  },
  "homepage": "./",
  "optionalDependencies": {
    "ajv": "6.10.2",
    "typescript": "3.5.3"
  },
  "devDependencies": {
    "electron": "^6.0.12",
    "electron-builder": "^21.2.0",
    "eslint-config-prettier": "6.0.0",
    "eslint-plugin-prettier": "3.1.0",
    "foreman": "^3.0.1",
    "gulp": "4.0.2",
    "gulp-append-prepend": "1.0.8",
    "prettier": "1.18.2"
  },
  "build": {
    "appId": "toggl.overtime.tracker",
    "files": [
      "build /**/*",
      "node_modules /**/*"
    ],
    "win": {
      "target": "msi",
      "icon": "build/icon.ico"
    },
    "directories": {
      "buildResources": "assets"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": []
  }
}